                            <li class="general-hamburguer-list__element c14-b">
                              <details>
                                <summary class="general-hamburguer-list__summary position-relative transition-opacity-03">
                                  <div class="d-flex justify-content-between align-items-center responsive-padding position-relative">
                                    <p class="d-block general-hamburguer-list__link body-text lh-normal c1-c fs-16-16 tw-normal">{{ link.title }}</p>
                                    <div class="position-relative">
                                      <div class="general-hamburguer-list__svg primary transition-opacity-03">
                                        <img loading="lazy" width="27" height="27" class="lozad" data-src="{{ 'smart-icon-more.svg' | asset_url }}" alt="Ver más">
                                      </div>
                                      <div class="general-hamburguer-list__svg secondary transition-opacity-03 position-absolute">
                                        <img loading="lazy" width="27" height="27" class="lozad" data-src="{{ 'smart-icon-close.svg' | asset_url }}" alt="Ver más">
                                      </div>
                                    </div>
                                  </div>
                                </summary>
                                <ul class="general-hamburguer-sublist__container list-unstyled c9-bk">
                                  {% for child_link in link.links %}
                                    {% if link.levels >= 2 %}
                                      {% for sub_child_link in child_link.links %}
                                        <li class="general-hamburguer-sublist__element responsive-padding">
                                          <a class="text-decoration-none d-flex align-items-center" href="{{ sub_child_link.url }}">
                                            <p class="body-text lh-normal c3-c fs-15-15">{{ sub_child_link.title }}</p>
                                          </a>
                                        </li>
                                      {% endfor %}
                                    {% else %}
                                    <li class="general-hamburguer-sublist__element responsive-padding">
                                      <a class="text-decoration-none d-flex align-items-center" href="{{ child_link.url }}">
                                        <p class="body-text lh-normal c3-c fs-15-15">{{ child_link.title }}</p>
                                      </a>
                                    </li>
                                    {% endif %}
                                  {% endfor %}
                                </ul>
                              </details>
                            </li>
                             {% comment %} FIN dónde se muestra, el título de boutique en mobile? {% endcomment %}
                          {% endif %}
                      {% endfor %}
                      </ul>
    
    
    
                      <div class="tw-px-4 tw-py-12 tw-border-t-[1px] tw-border-0 tw-border-solid tw-border-gray-200">
                        <div class="tw-text-center tw-mb-8">
                          <h2 class="tw-font-poppins tw-text-[#292929] tw-text-lg tw-font-semibold">{{section.settings.newsletter_title}}</h2>
                        </div>
                        {% form 'customer', id: 'ContactHeader', class: 'general-hamburguer__space-top' %}
                          <input type="hidden" name="contact[tags]" value="newsletter">
                          <div class="tw-flex tw-justify-center tw-items-center tw-w-full">
                            <div class="tw-flex tw-w-full tw-flex-col tw-gap-6">
                              <input
                                id="NewsletterForm--{{ section.id }}"
                                type="email"
                                name="contact[email]"
                                class="tw-font-poppins tw-text-[#292929] tw-text-base rounded tw-py-4 tw-px-4 tw-border tw-border-solid tw-border-gray-600"
                                value="{{ form.email }}"
                                aria-required="true"
                                autocorrect="off"
                                autocapitalize="off"
                                autocomplete="email"
                                {% if form.errors %}
                                  autofocus
                                  aria-invalid="true"
                                  aria-describedby="ContactFooter-error"
                                {% elsif form.posted_successfully? %}
                                  aria-describedby="ContactFooter-success"
                                {% endif %}
                                placeholder="{{ 'newsletter.label' | t }}"
                                required
                              >
                              <label class="visually-hidden" for="NewsletterForm--{{ section.id }}">{{ 'newsletter.label' | t }}</label>
                              <button class="tw-font-poppins tw-text-white tw-bg-amarillo tw-text-base tw-font-semibold tw-tracking-wide tw-uppercase tw-rounded tw-py-3 tw-px-4" type="submit" name="commit" id="Subscribe">
                                {{ 'newsletter.button_label' | t }}
                              </button>
                            </div>
                          </div>
                          {% if form.errors %}
                            <div class="text-center general-hamburguer__space-top">
                              <h3 class="tw-font-poppins tw-text-red-600 tw-text-base md:tw-text-[16px] tw-font-normal">¡Ups! Ocurrió un error. ¡Intenta una vez más!</h3>
                            </div>
                          {% endif %}
                          {% if form.posted_successfully? %}
                            <div class="text-center general-hamburguer__space-top">
                              <h3 class="tw-font-poppins tw-text-[#434f5a] tw-text-base md:tw-text-[16px] tw-font-normal">¡Gracias por formar parte de la familia Smart Bamboo!</h3>
                            </div>
                          {% endif %}
                        {% endform %}
                      </div>
