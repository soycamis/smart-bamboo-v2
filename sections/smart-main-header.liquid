<style>
    .degraded-builder { animation-name: building-degrade; animation-duration: 2s;}
    .asset-builder { animation-name: building-assets; animation-duration: 3s;}
    .appear-builder { animation-name: building-appear; animation-duration: 4s;}
    
    @keyframes building-degrade {
      0%   {display: none; opacity: 0;}
      25%  {display: none; opacity: 0;}
      50%  {display: block; opacity: 0;}
      100% {display: block; opacity: 1;}
    }  
    
    @keyframes building-assets {
      0%   {display: none;opacity: 0;}
      67%  {display: none; opacity: 0;}
      100% {display: block;opacity: 1;}
    }
    
    @keyframes building-appear {
      0%   {display: none; opacity: 0;}
      25%  {display: none; opacity: 0;}
      25%  {display: none; opacity: 0;}
      75%  {display: block; opacity: 0;}
      100% {display: block; opacity: 1;}
    }
    </style> 
    
